@model IEnumerable<KolpackiRWypozyczalnia.Models.Film>
@{
    
}

<table class="table">
    <thead>
        <tr>
            <th>Tytuł filmu</th>
            <th>Reżyser</th>
            <th>Opis</th>
            <th>Cena</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var film in Model)
        {
            <tr>
                <td> @film.Title </td>
                <td> @film.Director </td>
                <td> @film.Desc </td>
                <td> @film.Price </td>
            </tr>
        }
    </tbody>

</table>